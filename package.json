{
  "name": "ptsup",
  "description": "Build a packager with bundle or run tsc directories, suitable for node|browser library package processing, It is implemented using esbuild",
  "version": "0.1.0",
  "main": "index.ts",
  "keywords": [
    "tsup",
    "package.json",
    "mkdist",
    "build",
    "esbuild",
    "bundle",
    "pkg",
    "package",
    "rollup",
    "pnpm",
    "monorepo"
  ],
  "bin": {
    "ptsup": "bin/index.js"
  },
  "publishConfig": {
    "main": "./index.js",
    "types": "./index.d.ts",
    "directory": "dist"
  },
  "scripts": {
    "build": "ptsup -r src --dts --meta --clean",
    "change": "bumpp"
  },
  "license": "MIT",
  "dependencies": {
    "@types/fs-extra": "^9.0.13",
    "bumpp": "^8.2.1",
    "cac": "^6.7.12",
    "esbuild": "^0.14.46",
    "esbuild-register": "^3.3.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "lnk": "^1.1.0",
    "lodash": "^4",
    "pascal-case": "^3.1.2",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-dts": "^4.2.2",
    "slash": "3.0.0",
    "typescript": "^4.7.4",
    "unconfig": "^0.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@types/lodash": "^4",
    "@types/node": "^18.0.0",
    "eslint": "^8.18.0"
  }
}
